[rerere]
	enabled = true
	autoupdate = true
[branch]
	sort = -committerdate
[column]
	ui = auto
[tag]
	sort = version:refname
[init]
	defaultBranch = main
[diff]
	algorithm = histogram
	colorMoved = plain
	mnemonicPrefix = true
	renames = true
[push]
	autoSetupRemote = true
	followTags = true
[fetch]
	pruneTags = true
	all = true
	prune = true
[help]
	autocorrect = prompt
[commit]
	verbose = true
[rebase]
	autoStash = true
	autoSquash = true
	updateRefs = true
[merge]
	conflictstyle = zdiff3
[pull]
	rebase = true
